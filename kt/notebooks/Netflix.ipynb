    "// using the latest title added to remark the time when this\n",
    "// dataset were published, and to compute below statistics.\n",
    "val maxDate = df.date_added.max()\n",
    "val df_days = df.add {\n",
    "    \"days_on_platform\" from { date_added.daysUntil(maxDate) }\n",
    "    \"months_on_platform\" from { date_added.monthsUntil(maxDate) }\n",
    "    \"years_on_platform\" from { date_added.yearsUntil(maxDate) }\n",
    "}\n",
    "df_days.tail(2)[\"title\", \"days_on_platform\", \"months_on_platform\", \"years_on_platform\"]"
   ]
